<?xml version="1.0" encoding="utf-8" ?>

<wsdl:definitions
     name="DummyExposeWsSOAP11"
     targetNamespace="http://capacitacion.com.pe/ws/soap/util/peliculas"
     xmlns:tns="http://capacitacion.com.pe/ws/soap/util/peliculas"
     xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/"
     xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
     xmlns:types="http://capacitacion.com.pe/ws/soap/util/peliculas/types"   
     xmlns:http="http://schemas.xmlsoap.org/wsdl/http/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema"
     xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/"
     xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
     xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/" >
     
    <wsdl:documentation>Version 1.0</wsdl:documentation> 
          
	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://capacitacion.com.pe/ws/soap/util/peliculas/types"
				        schemaLocation="uti-peliculas.xsd" /> 
		</xsd:schema>
	</wsdl:types>
      
    
    <!-- ELEMENTO: REQUEST/RESPONSE (BODY) -->   
    <wsdl:message name="ConsultaPeliculaBodyRequest" >
        <wsdl:part name="consultaPeliculaBodyRequest" element="types:ConsultaPeliculaBodyRequest" /> 
    </wsdl:message>
 
    <wsdl:message name="ConsultaPeliculaBodyResponse" >
        <wsdl:part name="consultaPeliculaBodyResponse" element="types:ConsultaPeliculaBodyResponse" /> 
    </wsdl:message>
 
 
    <!-- PORT-TYPE -->
    <wsdl:portType name="ConsultaPeliculaPortTypeSOAP11" >
        <wsdl:operation name="consultaPelicula" >
            <wsdl:input  name="consultaPeliculaBodyRequest"   message="tns:ConsultaPeliculaBodyRequest" />
            <wsdl:output name="consultaPeliculaBodyResponse"  message="tns:ConsultaPeliculaBodyResponse" />    
        </wsdl:operation>
    </wsdl:portType>
    
    <wsdl:portType name="ConsultaPeliculaPortTypeSOAP12" >
        <wsdl:operation name="consultaPelicula" >
            <wsdl:input  name="consultaPeliculaBodyRequest"   message="tns:ConsultaPeliculaBodyRequest"  />   
            <wsdl:output name="consultaPeliculaBodyResponse"  message="tns:ConsultaPeliculaBodyResponse" />  
        </wsdl:operation>
    </wsdl:portType>    
            
        
    <!-- BINDING -->
    <wsdl:binding name="ConsultaPeliculaBindingSOAP11" type="tns:ConsultaPeliculaPortTypeSOAP11" >
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" /> 
        <wsdl:operation name="consultaPelicula" >
            <soap:operation soapAction="http://capacitacion.com.pe/ws/soap/util/peliculas/consultaPelicula" />
            <wsdl:input> 
                <soap:body use="literal" />
            </wsdl:input> 
            <wsdl:output> 
                <soap:body use="literal" />
            </wsdl:output> 
        </wsdl:operation>
    </wsdl:binding>    
    
    <wsdl:binding name="ConsultaPeliculaBindingSOAP12" type="tns:ConsultaPeliculaPortTypeSOAP12" >  
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" /> 
        <wsdl:operation name="consultaPelicula" >
            <soap12:operation soapAction="http://capacitacion.com.pe/ws/soap/util/peliculas/consultaPelicula" />
            <wsdl:input> 
                <soap12:body use="literal" />
            </wsdl:input> 
            <wsdl:output> 
                <soap12:body use="literal" />
            </wsdl:output> 
        </wsdl:operation>
    </wsdl:binding>      
            
            
    <!-- SERVICE -->
    <wsdl:service name="ConsultaPeliculaService" > 
    
        <wsdl:port name="ConsultaPeliculaPortSOAP11" binding="tns:ConsultaPeliculaBindingSOAP11" >
            <soap:address location="http://XXX:XXX/consultapeliculasoap11" />
        </wsdl:port>
        
        <wsdl:port name="ConsultaPeliculaPortSOAP12" binding="tns:ConsultaPeliculaBindingSOAP12" >  
            <soap12:address location="http://XXX:XXX/consultapeliculasoap12" />
        </wsdl:port>
    </wsdl:service>
    
</wsdl:definitions>

